<mds:customization
  version="12.2.1_20151011.0031"
  xmlns:mds="http://xmlns.oracle.com/mds">
  <mds:insert
    after="(xmlns(mds_ns1=http://xmlns.oracle.com/bc4j))/mds_ns1:ViewObject[@Name='EmployeesView']/mds_ns1:ViewAttribute[@Name='DepartmentId']"
    parent="(xmlns(mds_ns1=http://xmlns.oracle.com/bc4j))/mds_ns1:ViewObject[@Name='EmployeesView']">
    <ViewCriteria
      Name="EmployeesViewCriteria"
      ViewObjectName="com.kuchi.main.model.views.EmployeesView"
      Conjunction="AND"
      xmlns="http://xmlns.oracle.com/bc4j">
      <Properties Name="_Props">
        <CustomProperties>
          <Property
            Name="displayOperators"
            Value="InAdvancedMode"/>
          <Property
            Name="autoExecute"
            Value="false"/>
          <Property
            Name="allowConjunctionOverride"
            Value="true"/>
          <Property
            Name="showInList"
            Value="true"/>
          <Property
            Name="mode"
            Value="Basic"/>
        </CustomProperties>
      </Properties>
      <ViewCriteriaRow
        Name="vcrow0"
        UpperColumns="1">
        <ViewCriteriaItem
          Name="Salary"
          ViewAttribute="Salary"
          Operator=">="
          Conjunction="AND"
          Value="1000"
          Required="Optional"/>
      </ViewCriteriaRow>
    </ViewCriteria>
  </mds:insert>
  <mds:insert
    after="(xmlns(mds_ns1=http://xmlns.oracle.com/bc4j))/mds_ns1:ViewObject[@Name='EmployeesView']/mds_ns1:ViewAttribute[@Name='DepartmentId']"
    parent="(xmlns(mds_ns1=http://xmlns.oracle.com/bc4j))/mds_ns1:ViewObject[@Name='EmployeesView']">
    <ViewAttribute
      Name="CUSTOM_LAST_NAME"
      FromCustomization="true"
      IsSelected="false"
      IsPersistent="false"
      PrecisionRule="true"
      Precision="50"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="VIEW_ATTR"
      SDOHidden="true"
      SQLType="VARCHAR"
      xmlns="http://xmlns.oracle.com/bc4j">
      <TransientExpression
        trustMode="untrusted"
        Name="ExpressionScript">
        <![CDATA[return "Custom " + LastName;]]>
      </TransientExpression>
      <Properties>
        <SchemaBasedProperties>
          <LABEL
            ResId="CUSTOM_LAST_NAME_"/>
          <DISPLAYWIDTH
            Value="10"/>
        </SchemaBasedProperties>
      </Properties>
    </ViewAttribute>
  </mds:insert>
  <mds:modify
    element="(xmlns(mds_ns1=http://xmlns.oracle.com/bc4j))/mds_ns1:ViewObject[@Name='EmployeesView']/mds_ns1:ViewAttribute[@Name='FirstName']/mds_ns1:Properties/mds_ns1:SchemaBasedProperties/mds_ns1:LABEL">
    <mds:attribute
      name="ResId"
      value="NAME_FIRST"/>
  </mds:modify>
  <mds:insert
    parent="(xmlns(mds_ns1=http://xmlns.oracle.com/bc4j))/mds_ns1:ViewObject[@Name='EmployeesView']/mds_ns1:ViewAttribute[@Name='FirstName']"
    position="last">
    <Properties xmlns="http://xmlns.oracle.com/bc4j">
      <SchemaBasedProperties>
        <LABEL
          ResId="NAME_FIRST"/>
      </SchemaBasedProperties>
    </Properties>
  </mds:insert>
  <mds:insert
    parent="(xmlns(mds_ns1=http://xmlns.oracle.com/bc4j))/mds_ns1:ViewObject[@Name='EmployeesView']/mds_ns1:ViewAttribute[@Name='ManagerId']"
    position="last">
    <Properties xmlns="http://xmlns.oracle.com/bc4j">
      <SchemaBasedProperties>
        <LABEL
          ResId="MANAGER_ID"/>
      </SchemaBasedProperties>
    </Properties>
  </mds:insert>
</mds:customization>

